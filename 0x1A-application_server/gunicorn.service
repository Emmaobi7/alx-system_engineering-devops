[Unit]
Description=Gunicorn instance to serve airbnb     After=network.target

[Service]                                         User=ubuntu                                       Group=www-data                                    WorkingDirectory=/home/ubuntu/AirBnB_clone_v4/    ExecStart=/home/ubuntu/.local/bin/gunicorn --workers 3 --bind 0.0.0.0:5003 -m 007 web_dynamic.2-hbnb:app                                              StandardError=append:/tmp/airbnb-error.log        StandardOutput=append:/tmp/airbnb-access.log                                                        [Install]
WantedBy=multi-user.target
